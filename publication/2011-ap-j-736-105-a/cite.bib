@article{2011ApJ...736..105A,
 abstract = {The variety of isotopes in cosmic rays allows us to study different
aspects of the processes that cosmic rays undergo between the
time they are produced and the time of their arrival in the
heliosphere. In this paper, we present measurements of the
isotopic ratios $^2$H/$^4$He, $^3$He/$^4$He,
$^6$Li/$^7$Li, $^7$Be/($^9$Be+$^10$Be), and
$^10$B/$^11$B in the range 0.2-1.4 GeV of kinetic energy per
nucleon. The measurements are based on the data collected by the
Alpha Magnetic Spectrometer, AMS-01, during the STS-91 flight in
1998 June.},
